<div id="modalFileSystem" class="modal fade" tabindex="-1" aria-labelledby="modalFileSystemLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="modalFileSystemLabel">{{ modal_filesystem_title }}</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="{{ button_close }}"></button>
      </div>
      <div class="modal-body">
        <span id="modalFileSystemBody" data-url="{{ url_size_system }}"></span>
      </div>
      <div class="modal-footer">
        <button id="modalFileSystemRefresh" type="button" class="btn btn-primary">{{ button_refresh }}</button>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">{{ button_close }}</button>
      </div>
    </div>
  </div>
</div>
